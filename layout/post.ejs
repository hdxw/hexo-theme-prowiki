<%- partial('_widget/post_head', {post:page}) %>
<hr>
<article class="article-content">
    <%- partial('_widget/gallery') %>
    <%- page.content %>
</article>
<% if(page.content && page.content.length > 0){ %>
<aside class="article-toc" <% if (page.encrypt == true) { %>style="display:none" <% } %>>
<% if (page.encrypt == true) { %>
    <%- toc(page.origin, {list_number: theme.toc.list_number}) %>
<% } else { %>
    <%- toc(page.content, {list_number: theme.toc.list_number}) %>
<% } %>
</aside>
<% } %>
